    <sql id="selectSingleHistoryInfoVo">
        select ecg_id, patient_phone, normal_ecg, ap_beat, pv_beat, atrial_fibrillation, atrial_flutter, left_block, right_block, bradycardia, tachycardia from single_history_info
    </sql>
    <select id="selectSingleHistoryInfoList" parameterType="SingleHistoryInfo" resultMap="SingleHistoryInfoResult">
        <include refid="selectSingleHistoryInfoVo"/>
        <where>
            <if test="patientPhone != null  and patientPhone != ''"> and patient_phone = #{patientPhone}</if>
        </where>
    </select>

    <sql id="selectPatientManagementVo">
        select m.p_id, m.patient_code, m.hospital_code, m.equipment_code,
               connection_time, bed_number, case_history_number,
               ecg_type, online_status, patient_name, patient_age, patient_sex, patient_source,
               m.patient_phone, family_phone,  monitoring_status,
               equipment_status, equipment_type, hospital_name, intelligent_diagnosis,
               diagnosis_status, diagnosis_conclusion, diagnosis_doctor, report_time, ecg_level, doctor_phone,
               birth_day, pacemaker, upload_time, guardianship_duration, sampling_rate, detection_state,
               end_time, time_duration, d_phone, report_state,patient_symptom
        from patient_management m
                 inner join patient p on p.patient_phone = m.patient_phone
                 inner join equipment e on m.equipment_code = e.equipment_code
                 left join hospital h on m.hospital_code = h.hospital_code
                 left join report r on m.p_id = r.p_id
    </sql>

    <sql id="selectPatientManagementCount">
        select count(0)
        from patient_management m
        left join report r on m.p_id = r.p_id
        <if test="patientName != null or patientCode != null ">
            inner join patient p on p.patient_phone = m.patient_phone
        </if>
        <if test="hospitalName != null ">
            left join hospital h on m.hospital_code = h.hospital_code
        </if>
        <if test="bedNumber != null ">
            inner join equipment e on m.equipment_code = e.equipment_code
        </if>
    </sql>

    <select id="selectPatientManagementList" parameterType="PatientManagement" resultMap="PatientManagementResult">
        <include refid="selectPatientManagementVo"/>
        <where>
            <if test="pId != null  and pId != ''"> and m.p_id = #{pId}</if>
            <if test="patientName != null  and patientName != ''">and patient_name like concat('%', #{patientName}, '%')</if>
            <if test="patientCode != null  and patientCode != ''">and m.patient_code like concat('%', #{patientCode}, '%')</if>
            <if test="equipmentCode != null  and equipmentCode != ''">and m.equipment_code like concat('%', #{equipmentCode}, '%')</if>
            <if test="params.beginConnectionTime != null and params.beginConnectionTime != '' and params.endConnectionTime != null and params.endConnectionTime != ''"> and connection_time between #{params.beginConnectionTime} and #{params.endConnectionTime}</if>
            <if test="bedNumber != null  and bedNumber != ''">and bed_number like concat('%', #{bedNumber}, '%')</if>
            <if test="caseHistoryNumber != null  and caseHistoryNumber != ''">and case_history_number like concat('%', #{caseHistoryNumber}, '%')</if>
            <if test="ecgType != null  and ecgType != ''"> and ecg_type like concat(#{ecgType}, '%') </if>
            <if test="onlineStatus != null  and onlineStatus != ''"> and online_status = #{onlineStatus}</if>
            <if test="hospitalName != null  and hospitalName != ''"> and hospital_name = #{hospitalName}</if>
            <if test="patientPhone != null  and patientPhone != ''"> and m.patient_phone = #{patientPhone}</if>
            <if test="intelligentDiagnosis != null  and intelligentDiagnosis != ''"> and intelligent_diagnosis like concat('%', #{intelligentDiagnosis}, '%')</if>
            <if test="diagnosisStatus != null "> and diagnosis_status = #{diagnosisStatus}</if>
            <if test="diagnosisConclusion != null  and diagnosisConclusion != ''"> and diagnosis_conclusion like concat('%', #{diagnosisConclusion}, '%')</if>
            <if test="diagnosisDoctor != null  and diagnosisDoctor != ''"> and diagnosis_doctor like concat('%', #{diagnosisDoctor}, '%')</if>
            <if test="params.beginReportTime != null and params.beginReportTime != '' and params.endReportTime != null and params.endReportTime != ''"> and report_time between #{params.beginReportTime} and #{params.endReportTime}</if>
            <if test="ecgLevel != null "> and ecg_level = #{ecgLevel}</if>
            <if test="doctorPhone != null  and doctorPhone != ''"> and d_phone = #{doctorPhone}</if>
            <if test="pacemaker != null  and pacemaker != ''"> and pacemaker = #{pacemaker}</if>
            <if test="uploadTime != null "> and upload_time = #{uploadTime}</if>
            <if test="guardianshipDuration != null "> and guardianship_duration = #{guardianshipDuration}</if>
            <if test="detectionState != null">and detection_state = #{detectionState}</if>
            <if test="endReportTime != null and endReportTime != ''"> and date_format(connection_time, '%Y-%m-%d') = #{endReportTime}</if>
            <if test="equipmentCodeList != null and equipmentCodeList.size()>0">
                and  m.equipment_code in
                <foreach collection="equipmentCodeList" index="index" item="item" separator="," open="(" close=")">
                    #{item}
                </foreach>
            </if>
            <if test="hospitalCodeList != null and hospitalCodeList.size()>0">
                and  m.hospital_code in
                <foreach collection="hospitalCodeList" index="index" item="item" separator="," open="(" close=")">
                    #{item}
                </foreach>
            </if>

        </where>
        order by online_status desc, connection_time desc
    </select>
