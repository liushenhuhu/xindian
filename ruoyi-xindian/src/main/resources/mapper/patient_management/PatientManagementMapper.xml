    <select id="selectAllDateList" resultType="com.ruoyi.xindian.patient_management.vo.DateListVO">
        select   DATE_FORMAT(connection_time,'%Y-%m-%d') as date,COUNT(connection_time) as info
        from patient_management m
            inner join patient p on p.patient_phone = m.patient_phone
            inner join equipment e on m.equipment_code = e.equipment_code
            left join hospital h on m.hospital_code = h.hospital_code
            left join report r on m.p_id = r.p_id
      <where>
        <if test="ecgType != null  and ecgType != ''"> and ecg_type like concat(#{ecgType}, '%') </if>
        <if test="patientPhone != null  and patientPhone != ''"> and m.patient_phone = #{patientPhone}</if>
    </where>
        GROUP BY  DATE_FORMAT(connection_time,'%Y-%m-%d')
    </select>
